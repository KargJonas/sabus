{
  "name": "flatbuffers-example",
  "private": true,
  "type": "module",
  "scripts": {
    "gen": "PATH=\"./flatbuffers:./flatbuffers/build:../../build:../../flatbuffers/build:$PATH\" flatc --ts --gen-mutable -o generated schema/*.fbs",
